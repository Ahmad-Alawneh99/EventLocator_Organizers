<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eventlocator.eventlocatororganizers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EventLocatorOrganizers">
        <activity android:name=".ui.ViewEventActivity"></activity>
        <activity
            android:name=".ui.CreateEventActivity"
            android:label="Create Event">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.EventsActivity"
            android:label="Events"></activity>
        <activity
            android:name=".ui.ChangePasswordActivity"
            android:label="Change Password"></activity>
        <activity
            android:name=".ui.UpdateEmailActivity"
            android:label="Update Email"></activity>
        <activity
            android:name=".ui.IndividualEditProfileActivity"
            android:label="Edit Profile"></activity>
        <activity
            android:name=".ui.OrganizationEditProfileActivity"
            android:label="Edit Profile"></activity>
        <activity
            android:name=".ui.ProfileActivity"
            android:label="Profile" />
        <activity
            android:name=".ui.IndividualSetUpProfileActivity"
            android:label="Setup Your Profile"></activity>
        <activity
            android:name=".ui.OrganizationSetUpProfileActivity"
            android:label="Setup Your Profile"></activity>
        <activity android:name=".ui.LoginActivity"></activity>
        <activity
            android:name=".ui.SignUpActivity"
            android:label="Sign Up"></activity>
        <activity
            android:name=".ui.WelcomeActivity"
            android:label="Welcome" />
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>